import java.util.Scanner;
public class studyopedia {
                 public static void main(String[] args) {
                Scanner scan = new Scanner(System.in);
                int a =scan.nextInt();
                 int b =scan.nextInt();
                 int c=scan.nextInt();
                 int d=a+b+c;
                 if(d>=180){   
System.out.println("triangle is formed")
}
              else{  System.out.println("triangle is not formed");
               }
}
}

